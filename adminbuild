// This program reads line by line from a text file
#include <fstream> // 
#include <iostream>
#include <string>
using namespace std;


int main()
{
	ifstream file;
	string text;
	int number;

	//open the input file
	file.open("data.txt");

	//if file is not empty, read line by line
	while (getline(file, text))
	{
		cout << text << endl;
	}
		file.close();
	system("pause");

	return 0;
}
